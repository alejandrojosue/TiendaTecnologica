{
  "kind": "collectionType",
  "collectionName": "facturacions",
  "info": {
    "singularName": "facturacion",
    "pluralName": "facturacions",
    "displayName": "Facturacion",
    "description": ""
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {},
  "attributes": {
    "RangoInicial": {
      "type": "integer",
      "default": 1,
      "required": true,
      "min": 1
    },
    "RangoFinal": {
      "type": "integer",
      "default": 2,
      "required": true,
      "min": 2
    },
    "CAI": {
      "type": "string",
      "required": true,
      "unique": true,
      "maxLength": 14,
      "minLength": 14,
      "regex": "^[a-zA-Z0-9]+$"
    },
    "fechaInicio": {
      "type": "date",
      "required": true
    },
    "fechaFin": {
      "type": "date",
      "required": true
    },
    "sucursal": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::sucursal.sucursal"
    }
  }
}
